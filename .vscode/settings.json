{
  "[solidity]": {
    "editor.defaultFormatter": "JuanBlanco.solidity"
  },
  "editor.formatOnSave": true,
  "makefile.extensionOutputFolder": "./cache",
  "prettier.documentSelectors": [
    "**/*.sol"
  ],
  "search.exclude": {
    "lib": true
  },
  "solidity.compileUsingRemoteVersion": "v0.8.19+commit.7dd6d404",
  "solidity.formatter": "prettier",
  "solidity.packageDefaultDependenciesContractsDirectory": "src",
  "solidity.packageDefaultDependenciesDirectory": "lib",
  "solidity.remappings": [
    "@chainlink=lib/chainlink/contracts/src/v0.8/",
    "@chainlink/vrf/=lib/chainlink/contracts/src/v0.8/vrf/",
    "@solidstate/contracts/=lib/solidstate-solidity/contracts/",
    "forge-safe/=lib/forge-safe/src",
    "forge-std/=lib/forge-std/src/"
  ]
}